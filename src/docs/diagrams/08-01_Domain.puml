@startuml

class Route {
    String name
    String _description
}

class GeoCoordinate {
    double latitude
    double longitude
    double _elevation
}

User "1" -- "*" Route : has
User "1" -- "*" User : friends
Route -> "*" GeoCoordinate
Route -left-> "*" Comment
Comment -up-> "1" User
Route -down-> "*" Image
Route -down-> "*" Video

@enduml